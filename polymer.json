{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/pages/page-view1.html",
    "src/pages/page-view2.html",
    "src/pages/page-view3.html",
    "src/pages/page-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds":[{
    "bundle": false,
    "js": {"minify": false},
    "css": {"minify": false},
    "html": {"minify": false},
    "addServiceWorker": true,
    "addPushManifest": true,
    "insertPrefetchLinks": false
  }]
}
